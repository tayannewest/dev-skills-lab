<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Express Dev Skills Lab</title>
</head>
<body>
  <h1>Skills as of <%= time %> </h1>
  <% if(error) { %>
    <h2><%= error %></h2>
  <% } %>
  <ul>
    <% skills.forEach(function(skill) { %>
      <li>
        <a href="/skills/<%= skill._id %>"><%= skill.text %></a>
        -
        <%= skill.has ? 'does have' : 'does not have' %>
      </li>
    <% }) %>
  </ul>
</body>
</html>